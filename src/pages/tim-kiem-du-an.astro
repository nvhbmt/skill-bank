---
import DefaultLayout from '@/layouts/DefaultLayout.astro';
import '@/assets/css/tim-kiem-du-an.css';
import '@/assets/script/tim-kiem-du-an.js';
---

<DefaultLayout>
    <header>
        <div class="header-container">
            <div class="left-header">
                <img src="./assets/tim-kiem-du-an/logo-full-ngang.svg" alt="" />
                <button class="left-header-button">Bảng điều khiển</button>
                <img
                    src="./assets/tim-kiem-du-an/Icon-managerProject.svg"
                    alt=""
                />
            </div>
            <div class="right-header">
                <img src="./assets/tim-kiem-du-an/Icon-project.svg" alt="" />
                <img
                    src="./assets/tim-kiem-du-an/Icon-notification-default.svg"
                    alt=""
                />
                <button class="right-header-button">Đăng tải dự án</button>
                <div class="information">
                    <img src="./assets/tim-kiem-du-an/avt-nhỏ.svg" alt="" />
                    <div class="name-information">
                        <div class="username">User name</div>
                        <div class="email">Email</div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <div class="title">
        <h1>Khám phá dự án</h1>
    </div>
    <div class="search-container">
        <div class="search-bar">
            <div class="icon-search">
                <img src="./assets/tim-kiem-du-an/Icon-search.svg" alt="" />
            </div>
            <div class="filter">
                <img src="./assets/tim-kiem-du-an/mage_filter.svg" alt="" />
            </div>
            <input
                type="text"
                id="searchInput"
                placeholder="Nhập từ khoá dự án, kỹ năng..."
                onfocus="showSuggestions()"
                onblur="hideSuggestions() "
            />
        </div>
    </div>
    <div class="project">
        <div class="project-container">
            <div class="project-image">
                <img src="./assets/tim-kiem-du-an/project-image.svg" alt="" />
                <div class="project-content">
                    <div class="left-project-content">
                        <div class="design-web">Design web</div>
                        <div class="email-address">buithaivy2019@gmail.com</div>
                    </div>
                    <div class="right-project-content">
                        <div class="interact">
                            <div class="eyes">
                                <img
                                    src="./assets/tim-kiem-du-an/teenyicons_eye-outline.svg"
                                    alt=""
                                />
                                990
                            </div>
                            <div class="like">
                                <img
                                    src="./assets/tim-kiem-du-an/solar_like-outline.svg"
                                    alt=""
                                />
                                500
